{
  "name": "website-insights-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "remix dev --port 3000",
    "dev:backend": "convex dev",
    "start": "remix-serve build",
    "type-check": "tsc --noEmit",
    "type-check:convex": "tsc -p convex --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "convex:check": "convex dev --once",
    "pre-check": "npm-run-all --sequential type-check type-check:convex lint format:check build",
    "fix-all": "npm-run-all --sequential lint:fix format"
  },
  "dependencies": {
    "@remix-run/dev": "^2.16.8",
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "@remix-run/serve": "^2.16.8",
    "clsx": "^2.1.1",
    "convex": "^1.24.2",
    "googleapis": "^150.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "~10",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^7.0.2"
  }
}
